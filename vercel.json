{
  "env": {
      "FLASK_ENV": "production"
  },
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/api/static/$1"
      },
      {
          "src": "/templates/(.*)",
          "dest": "/api/templates/$1"
      },
      {
          "src": "/(.*)",
          "dest": "/api/index"
      }
  ]
}